{
  "name": "@evolvedbinary/prosemirror-lwdita",
  "version": "2.6.0",
  "description": "Use LwDITA in ProseMirror",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=22.16.0 <23",
    "yarn": ">=4.2.2"
  },
  "scripts": {
    "clean": "rimraf dist generated-docs tsconfig.tsbuildinfo",
    "fix": "node scripts/fix",
    "postinstall": "yarn fix",
    "build": "tsc",
    "prepack": "yarn run build",
    "test": "mocha",
    "test:watch": "mocha -w",
    "coverage": "rimraf coverage && nyc yarn mocha",
    "coveralls": "nyc --reporter=text-lcov report | coveralls",
    "generate-docs": "typedoc --options typedoc.config.mjs --entryPoints src/index.ts --out dist/doc",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evolvedbinary/prosemirror-lwdita.git"
  },
  "keywords": [
    "ProseMirror",
    "LwDITA",
    "DITA",
    "XDITA",
    "XML",
    "JDITA",
    "JSON"
  ],
  "author": "Evolved Binary",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/evolvedbinary/prosemirror-lwdita/issues"
  },
  "homepage": "https://github.com/evolvedbinary/prosemirror-lwdita#readme",
  "directories": {
    "src": "./src",
    "test": "./test",
    "scripts": "./scripts"
  },
  "dependencies": {
    "@evolvedbinary/lwdita-ast": "^3.3.0",
    "@evolvedbinary/lwdita-xdita": "^3.3.0",
    "@evolvedbinary/prosemirror-lwdita-localization": "workspace:^",
    "@types/chai-as-promised": "^8.0.2",
    "chai-as-promised": "^8.0.2",
    "fetch-mock": "^12.6.0",
    "prosemirror-commands": "^1.7.1",
    "prosemirror-history": "^1.4.1",
    "prosemirror-keymap": "^1.2.2",
    "prosemirror-menu": "^1.2.5",
    "prosemirror-model": "^1.25.4",
    "prosemirror-schema-list": "^1.4.1",
    "prosemirror-state": "^1.4.3",
    "prosemirror-transform": "^1.10.1",
    "prosemirror-view": "^1.41.3",
    "toastify-js": "^1.12.0",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.9",
    "@types/node": "^24.9.2",
    "@types/toastify-js": "^1",
    "@types/urijs": "^1.19.26",
    "chai": "^6.2.0",
    "coveralls": "^3.1.1",
    "eslint": "^9.38.0",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.10",
    "nyc": "^17.0.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.14",
    "typescript": "5.9.3"
  },
  "packageManager": "yarn@4.2.2"
}
