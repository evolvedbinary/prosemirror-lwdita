{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/server.ts"],
  "exclude": ["generated-docs/**/*.ts", "dist/**/*.ts", "node_modules/**/*.ts"],
  "out": "generated-docs",
  "treatWarningsAsErrors": true,
  "highlightLanguages": [
      "bash",
      "console",
      "css",
      "html",
      "javascript",
      "json",
      "jsonc",
      "json5",
      "tsx",
      "typescript",
      "xml"
  ],
  "plugin": ["typedoc-plugin-missing-exports"]
}
